source 'https://github.com/CocoaPods/Specs.git'
source 'https://bitbucket.scalefocus.com/scm/il/podspecrepo.git'

platform :ios, '13.0'

target 'ti-broish' do
  use_frameworks!

  pod 'TwoWayBondage'
  pod 'Alamofire', '~> 5.2'

  pod 'Firebase/Auth'
  pod 'Firebase/Messaging'

end

target 'ti-broishTests' do
  use_frameworks!

  pod 'TwoWayBondage'
  pod 'Alamofire', '~> 5.2'

  pod 'Firebase/Auth'
  pod 'Firebase/Messaging'

end
